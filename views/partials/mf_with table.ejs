<div class="mf-wrapper">
<section class="mf-content">
<header class="mf-container">
  <h1>Don't want to research or invest in equity.<br>Mutual Funds are the investment you are looking for.</h1>
  <p>We will help you to find the best fund available in the market.</p>
  
  <form class="form-wrapper cf col-lg-6 col-md-6 col-sm-6 col-xs-6" method="POST" ng-submit="searchthisstock()">
    <input type="text" name="query" placeholder="Search here for mutual funds...&nbsp;&nbsp;&nbsp;&nbsp;ex. Axis,Franklin templeton,UTI etc" ng-model="search_stock" required>
    <button type="submit" name="search" >Search</button>
</form>
</header>

<nav class="navbar-home hidden-xs">
  <div class="navbar-container">
  <a class="navbar-name">Investdia</a>
    <ul class="navbar-links">
    <li><a href="/home" class="home-link">HOME</a></li>
    <li><a href="#services" class="service-link">SERVICES</a></li>
     <li><a href="/blog" target="_blank" class="blog-link">BLOG</a></li>
    <li><a href="#about" class="about-link">ABOUT</a></li>
    <li><a href="#contact" class="contact-link">CONTACT</a></li>
    </ul>
  </div>
</nav>
</section>


<div class="mf-detail-container">
<section class="mf-filter">
<div class="filter-heading">
  <span>Filter Results</span>
</div>

<form name="filterForm" class="filter-form">

<!-- <div class="family-chkbox"> -->
<div class="fund-family-title">
<div>Fund Family</div>
<!-- <div class="expand-icon"><i class="fa fa-plus-square-o" ng-click="collapse = !collapse"></i></div> -->
<div class="filter-icon">
  <i class="fa fa-plus-square-o" ng-click="showfamily=true" ng-hide="showfamily"></i>
  <i class="fa fa-minus-square-o" ng-click="showfamily=false" ng-hide="!showfamily"></i>
</div>
</div>
<div class="mf_family_container">
<div ng-repeat = "family in familyunit" class="family_1" ng-show="showfamily">
    <label>
    <input type="checkbox" ng-click="includeFund(family.id)">
<div class="check-label">{{family.name}}</div>
  </label>
</div>
</div>

<!-- </div> -->
<div class="mf_filters">

<div class="filter-sub-heading">
<span>Mutual Fund Category</span>
</div>
<select ng-model="mfcateg" class="mf_categ" ng-options="category for category in mfcategory">
<option value="">All</option>
</select>

<div class="filter-sub-heading">
<span>Mutual Fund Type</span>
</div>
<select ng-model="mftype" -options="mtype for mtype in mftypes">
<option value="">All</option>
</select>

<div class="filter-sub-heading">
<span>Crisil Rating</span>
</div>
<select ng-model="mfrating" class="mf_rating" ng-options="crating for crating in crisilrating">
<option value="">All</option>
</select>

<div class="filter-sub-heading">
<span>Benchmark Index</span>
</div>
<select ng-model="fundbench" class="fund_bench" ng-options="index for index in benchmark">
<option value="">All</option>
</select>

<div class="filter-main-heading">
<span>Fund Features</span>
<div class="filter-icon">
  <i class="fa fa-plus-square-o" ng-click="show=true" ng-hide="show"></i>
  <i class="fa fa-minus-square-o" ng-click="show=false" ng-hide="!show"></i>
</div>
</div>
<div class="feature-filter-content" ng-show="show">
<div class="filter-sub-heading">
<span>Manager Experience (In years)</span>
</div>
<!-- <select ng-model="fundexp" class="fund_exp" ng-options="exp for exp in managerexp"> -->
<div class="mf-manager-exp">
<input class="min-text" ng-model="search.exp_min"/>
<input class="max-text" ng-model="search.exp_max"/>
<slider-range min="0" max="50" value-min="search.exp_min" value-max="search.exp_max"></slider-range>
<br>
</div>
<!-- </select> -->
<div class="filter-sub-heading">
<span>Inception Date (In years)</span>
</div>
<!-- <select ng-model="fundexp" class="fund_exp" ng-options="exp for exp in managerexp">
</select> -->
<div class="mf-incetion-since">
<input class="min-text" ng-model="incept_min"/>
<input class="max-text" ng-model="incept_max"/>
<slider-range min="0" max="50" value-min="incept_min" value-max="incept_max"></slider-range>
<br>
</div>

<div class="filter-sub-heading">
<span>Risk Index</span>
  <div class="risk-type" ng-switch on="risk">
    <div ng-switch-when=1>Low</div>
    <div ng-switch-when=2>Moderate Low</div>
    <div ng-switch-when=3>Moderate</div>
    <div ng-switch-when=4>Moderate High</div>
    <div ng-switch-when=5>High</div>
     <div ng-switch-default>All</div>
  </div>
</div>

  <form class="stock-option-form"> 
     <fieldset class="risk-set">
    <ul>
      <li><input type="radio" ng-model="risk" name="content" ng-value=1>
      <label for="radio1"></label>
      <li><input type="radio" ng-model="risk" name="content" ng-value=2>
      <label for="radio2"></label>
      <li><input type="radio" ng-model="risk" name="content" ng-value=3>
      <label for="radio3"></label>
      <li><input type="radio" ng-model="risk" name="content" ng-value=4>
      <label for="radio4"></label>
      <li><input type="radio" ng-model="risk" name="content" ng-value=5>
      <label for="radio5"></label>
      <li><input type="radio" ng-model="risk">
      <label for="radio6"></label>
    </ul>
  </fieldset>
    </form>


</div>
</div>
</form>
</section>

 <section class="mf-display">
 <div class="mf-block" ng-repeat = "mf in mf_schemes | filter:fundFilter"> 
    <div class="mf-details">
    <div class="mf_line1">
      <div class="line1_mf_name">{{mf.amc_name}}</div>
      <div class="line1_mf_number">{{filteredItems.length}} of {{mf.schemes.length}} schemes matching your criteria.</div>
    </div>

<!-- <ul class="mf-list" ng-class="grid-list"> -->

<table class="responsive">
    <thead>
      <tr>
      <th scope="col" style="width:20%">Scheme Name</th>
      <th scope="col" style="width:13%">Minimum Investment</th>
      <th scope="col" style="width:12%">Minimum SIP</th>
      <th scope="col" style="width:15%">Exit Load</th>
      <th scope="col" style="width:15%">Benchmark Index</th>
      <th scope="col" style="width:13%">Manager</th>
      <th scope="col" style="width:12%">Action</th>
<!--             <th scope="col">Scheme Name</th>
      <th scope="col">Minimum Investment</th>
      <th scope="col">Minimum SIP</th>
      <th scope="col">Exit Load</th>
      <th scope="col">Benchmark Index</th>
      <th scope="col">Manager</th>
      <th scope="col">Action</th> -->
      </tr>
    </thead>
    
    <tbody>

    <tr ng-repeat="scheme in filteredItems = (mf.schemes | filter:{scheme_category:mfcateg || undefined,scheme_type:mftype || undefined,rating:mfrating || undefined,riskindex:risk || undefined}:true |exp_Range:search.exp_min:search.exp_max | inception_Range:incept_min:incept_max ) | limitTo:limitmf">

          <td data-table-header="Scheme Name">{{scheme.scheme_name}}</td>
          <td data-table-header="Minimum Investment">{{scheme.scheme_minimum}}</td>
          <td data-table-header="Minimum SIP">3000</td>
      <td data-table-header="Exit Load"><div class="exit-load-spl">{{scheme.scheme_load}}</div></td>
      <td data-table-header="Benchmark Index">{{scheme.scheme_benchmark}}</td>
      <td data-table-header="Manager" ng-repeat = "manager in scheme.scheme_manager">{{manager.name}}</td>
      <td data-table-header="Action" class="action-btn"><button type="button" class="btn btn-success mf-start">Buy</button>
    <button type="button" class="btn btn-primary mf-start">SIP</button>  </td>
      
     
      </tr>
    </tbody>

</table>


