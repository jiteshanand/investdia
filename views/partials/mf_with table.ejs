<div class="no-record" ng-show="filteredItems.length == 0">No scheme found that matched  specified criteria.</div>
<div class="show-more" ng-click="limitmf = limitmf + (filteredItems.length-limitmf);display=true" ng-show="filteredItems.length > 3" ng-hide="display"><a href=""><i class="fa fa-plus-circle"></i> Show more</a>
</div>
<div class="show-less" ng-hide="!display" ng-show="filteredItems.length > 3" ng-click="limitmf = 3;display=false"><a href=""><i class="fa fa-minus-circle"></i> Show Less</a>
</div>

   </div>
 </div>
 </section>
 </div>

</div>